{
  "name": "flutter-hybrid-react-boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A flutter hybrid react boilerplate depend on inappwebview plugin",
  "repository": {
    "type": "git",
    "url": "git@github.com:posrix/flutter-hybrid-react-boilerplate.git"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@craco/craco": "^7.0.0-alpha.7",
    "@reduxjs/toolkit": "^1.8.2",
    "@tanstack/react-table": "^8.5.15",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.38",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "ahooks": "^3.5.0",
    "antd-mobile": "^5.13.1",
    "antd-mobile-icons": "^0.2.2",
    "big.js": "^6.2.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "debug": "^4.3.4",
    "immer": "^9.0.14",
    "lodash": "^4.17.21",
    "pinyin": "^3.0.0-alpha.4",
    "qs": "^6.11.0",
    "react": "^17.0.2",
    "react-activation": "^0.12.1",
    "react-dom": "^17.0.2",
    "react-is": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.3",
    "ua-parser-js": "^1.0.2",
    "web-vitals": "^2.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "start": "PORT=3080 API_HOST=dev BROWSER=none craco start",
    "start:web": "PORT=3080 API_HOST=dev WEB=true craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@faker-js/faker": "^7.5.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/big.js": "^6.1.5",
    "@types/debug": "^4.1.7",
    "@types/react-is": "^17.0.3",
    "@types/styled-components": "^5.1.25",
    "@types/ua-parser-js": "^0.7.36",
    "babel-plugin-named-exports-order": "^0.0.2",
    "conventional-changelog-cli": "^2.2.2",
    "craco-alias": "^3.0.1",
    "craco-less": "^2.1.0-alpha.0",
    "cz-conventional-changelog": "^3.3.0",
    "jszip": "^3.10.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mime-types": "^2.1.35",
    "prop-types": "^15.8.1",
    "ts-node": "^10.9.1",
    "vconsole": "^3.14.7",
    "webpack": "^5.73.0",
    "webpack-sources": "^3.2.3"
  }
}
